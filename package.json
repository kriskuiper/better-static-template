{
  "name": "static-ecommerce",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:kriskuiper/static-ecommerce.git",
  "author": "Kris Kuiper <kkuiper@enrise.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "prestart": "rimraf dist",
    "sass": "sass --no-source-map src/site/assets/scss/core.scss dist/styles/index.css",
    "postcss": "postcss dist/styles/index.css --no-map --replace",
    "start": "cross-env ELEVENTY_ENV=development run-p watch:*",
    "start:netlify": "netlify dev --port 5051",
    "watch:eleventy": "eleventy --serve",
    "watch:css": "sass --watch --no-source-map src/site/assets/scss/core.scss dist/styles/index.css",
    "watch:js": "rollup -c --watch",
    "build": "cross-env ELEVENTY_ENV=production run-s build:*",
    "build:eleventy": "eleventy",
    "build:css": "run-s sass postcss",
    "build:js": "rollup -c"
  },
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.4",
    "postcss-cli": "^8.1.0",
    "postcss-import": "^13.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.27.0"
  }
}
